{
  "private": true,
  "name": "hungrytapirldn.com",
  "license": "MIT",
  "scripts": {
    "postinstall": "remix-esbuild-override",
    "build": "concurrently \"npm run build:*\"",
    "build:remix": "remix build --sourcemap",
    "build:css": "tailwindcss -m -i ./app/styles/tailwind.css -o app/styles/app.css",
    "dev": "concurrently \"npm run dev:*\"",
    "dev:css": "tailwindcss -w -i ./app/styles/tailwind.css -o app/styles/app.css",
    "dev:remix": "remix watch",
    "dev:wrangler": "wrangler pages dev ./public --compatibility-date 2023-06-15 --live-reload true",
    "start": "npm run wrangler",
    "typecheck": "tsc",
    "test": "jest",
    "logs:test": "wrangler pages deployment tail --project-name hungrytapir-store --environment preview",
    "logs:prod": "wrangler pages deployment tail --project-name hungrytapir-store --environment production"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.8",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-menubar": "^1.0.3",
    "@radix-ui/react-navigation-menu": "^1.1.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@remix-run/cloudflare": "^1.19.0",
    "@remix-run/cloudflare-pages": "^1.19.0",
    "@remix-run/react": "^1.19.0",
    "@sentry/remix": "^7.60.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "dompurify": "^3.0.5",
    "imagekit": "^4.1.3",
    "imagekitio-react": "^2.0.0",
    "isbot": "^3.6.13",
    "lucide-react": "^0.263.1",
    "markdown-to-jsx": "^7.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stripe": "^12.4.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.19.0",
    "@jest/globals": "^27.5.1",
    "@remix-run/dev": "^1.19.0",
    "@remix-run/eslint-config": "^1.7.1",
    "@tailwindcss/forms": "^0.5.3",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "dotenv": "^16.1.4",
    "esbuild-plugin-replace": "^1.4.0",
    "eslint": "^8.38.0",
    "jest": "^25.0.0",
    "postcss": "^8.4.22",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "remix-esbuild-override": "^3.1.0",
    "stripe-event-types": "^2.3.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.2",
    "tailwindcss-animate": "^1.0.6",
    "typescript": "^4.9.5",
    "vitest": "^0.32.0",
    "wrangler": "^2.20.0"
  },
  "engines": {
    "node": ">=16.13"
  },
  "sideEffects": false
}
