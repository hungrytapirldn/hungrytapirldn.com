{
    "private": true,
    "sideEffects": false,
    "scripts": {
        "build": "concurrently \"npm run build:*\"",
        "build:remix": "remix build",
        "build:css": "tailwindcss -m -i ./app/styles/tailwind.css -o app/styles/app.css",
        "dev": "cross-env NODE_ENV=DEV concurrently \"npm run dev:*\"",
        "dev:css": "tailwindcss -w -i ./app/styles/tailwind.css -o app/styles/app.css",
        "dev:remix": "remix watch",
        "dev:wrangler": "wrangler pages dev ./public --live-reload true",
        "start": "npm run wrangler",
        "typecheck": "tsc"
    },
    "dependencies": {
        "@notionhq/client": "^2.2.5",
        "@radix-ui/react-aspect-ratio": "^1.0.3",
        "@radix-ui/react-checkbox": "^1.0.4",
        "@radix-ui/react-label": "^2.0.2",
        "@radix-ui/react-popover": "^1.0.6",
        "@radix-ui/react-slot": "^1.0.2",
        "@remix-run/cloudflare": "^1.16.0",
        "@remix-run/cloudflare-pages": "^1.16.0",
        "@remix-run/react": "^1.16.0",
        "class-variance-authority": "^0.6.0",
        "clsx": "^1.2.1",
        "cmdk": "^0.2.0",
        "cross-env": "^7.0.3",
        "isbot": "^3.6.8",
        "lucide-react": "^0.236.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "stripe": "^12.4.0",
        "tailwind-merge": "^1.13.0",
        "tailwindcss-animate": "^1.0.5"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^3.19.0",
        "@remix-run/dev": "^1.16.0",
        "@remix-run/eslint-config": "^1.16.0",
        "@tailwindcss/forms": "^0.5.3",
        "@tailwindcss/typography": "^0.5.9",
        "@types/react": "^18.0.35",
        "@types/react-dom": "^18.0.11",
        "autoprefixer": "^10.4.14",
        "concurrently": "^8.0.1",
        "eslint": "^8.38.0",
        "postcss": "^8.4.22",
        "prettier": "^2.8.8",
        "prettier-plugin-tailwindcss": "^0.2.8",
        "stripe-event-types": "^2.3.0",
        "tailwindcss": "^3.3.2",
        "typescript": "^4.9.5",
        "wrangler": "^2.20.0"
    },
    "engines": {
        "node": ">=16.13"
    }
}
